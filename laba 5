	for ( i = 0; i < m; i++)
	{
		int numb[i]= rand() % 10 -10;
	}
	for ( i = 0; i < n; i++)
	{
		real numb[i] = (((double)rand() * (1 - 0) / RAND_MAX) + 2);

	}

}
